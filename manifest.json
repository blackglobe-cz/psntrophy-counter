{
  "name": "PSN Profiles Counter",
  "version": "1.1",
  "description": "Extension that counts games within trophy advisor page.",
  "manifest_version": 3,
  "permissions": [
    "declarativeContent"
  ],
  "icons": {
    "16": "icons/icon-16.png",
    "48": "icons/icon-48.png",
    "128": "icons/icon-128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://psnprofiles.com/*log*"
      ],
      "js": [
        "jquery.js",
        "board.js"
      ]
    }
  ]
}
